{
  "name": "imicros-minio",
  "version": "0.0.4",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Moleculer service for minio object storage (with authentification by imicros-auth and authorization by imicros-acl)",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx",
    "test": "jest --coverage --no-cache",
    "test-local": "jest --coverage --no-cache --config=local.js",
    "test-unit": "jest test/minio.spec.js --no-cache --config=local.js",
    "test-mixin": "jest test/minio.mixin.spec.js --no-cache --config=local.js",
    "test-gateway": "jest test/gateway.spec.js --no-cache --config=local.js"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js",
      "!lib/globals.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-minio.git"
  },
  "keywords": [
    "imicros",
    "moleculer",
    "minio",
    "store"
  ],
  "devDependencies": {
    "busboy": "^0.3.1",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "moleculer": "^0.13.13",
    "nats": "^1.4.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "imicros-acl": "0.0.3",
    "lodash": "^4.17.15",
    "minio": "^7.0.14",
    "moleculer-web": "git+https://github.com/moleculerjs/moleculer-web.git",
    "uuid": "^3.4.0"
  }
}
